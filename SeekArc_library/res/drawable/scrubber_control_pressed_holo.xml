<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:gravity="center"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <size
                android:width="30dp"
                android:height="30dp" />
            <solid android:color="#4c33b5e5" />
            <stroke
                android:width="2dp"
                android:color="#33b5e5" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="8dp"
                android:height="8dp" />
            <solid android:color="#33b5e5" />
        </shape>
    </item>
</layer-list>